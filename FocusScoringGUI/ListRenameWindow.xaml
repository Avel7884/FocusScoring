<Window x:Class="FocusScoringGUI.ListRenameWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:FocusScoringGUI"
        mc:Ignorable="d"
        Title="ListRenameWindow" Height="140" Width="280">
    <Grid>
        <StackPanel>
            <TextBlock Text="Введите новое имя" Width="150" HorizontalAlignment="Center" Margin="5,5,5,5"/>
            <TextBox Name="NameBox" HorizontalAlignment="Center" Width="150" Margin="5,5,5,5"/>
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                <Button Click="OkButton_OnClick" Width="75" Margin="5,5,5,5">OK</Button>
                <Button Click="CanselButton_OnClick" Width="75" Margin="5,5,5,5">Отмена</Button>
            </StackPanel>
        </StackPanel>
    </Grid>
</Window>
